{
    "name": "GitHub",
    "type": "light",
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#202328",
        "editor.cursor": "#797979",
        "editor.selectionBackground": "#E4F9FF"
    },
    "tokenColors": [
        {
            "scope": ["Preprocessor Statements"],
            "settings": {
                "foreground": "#be3536"
            }
        },
        {
            "scope": ["Comments"],
            "settings": {
                "foreground": "#5b636d"
            }
        },
        {
            "scope": ["Keywords"],
            "settings": {
                "foreground": "#be3536"
            }
        },
        {
            "scope": ["Brackets"],
            "settings": {
                "foreground": "#202328"
            }
        },
        {
            "scope": ["Operators"],
            "settings": {
                "foreground": "#214fa8"
            }
        },
        {
            "scope": ["Numbers", "Strings", "Backquotes"],
            "settings": {
                "foreground": "#152f65"
            }
        },
        {
            "scope": ["Functions"],
            "settings": {
                "foreground": "#603bb3"
            }
        }
    ]
}
