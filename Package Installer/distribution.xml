<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>PrimeSDK</title>
    <options customize="allow" rootVolumeOnly="true" hostArchitectures="i386,arm64"/>
    <background file="background.tif" alignment="topleft" scaling="none"/>
    <welcome file="en.lproj/Welcome.rtf" mime-type="text/rtf" uti="public.rtf"/>
    <license file="en.lproj/License.rtf" mime-type="text/rtf" uti="public.rtf"/>
    <choices-outline>
        <line choice="default"/>
    </choices-outline>
    <choice id="default" title="Install PrimeSDK">
        <pkg-ref id="uk.insoft.primesdk"/>
    </choice>
    <pkg-ref id="uk.insoft.primesdk" installKBytes="4104" version="1.1" onConclusion="none">primesdk.pkg</pkg-ref>
</installer-gui-script>
